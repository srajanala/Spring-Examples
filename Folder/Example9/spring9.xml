<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
  <bean id="triangle" class="org.saisindhu.package9.Triangle">
     <property name="PointA" ref="pointA"/>
      <property name="PointB" ref="pointB"/>
      <property name="PointC" ref="pointC"/>
    
  </bean>

  <bean id="pointA" class="org.saisindhu.package9.Point">
   <property name="x" value="0"/>
   <property name="y" value="0"/>
  </bean>
  
  <bean id="pointB" class="org.saisindhu.package9.Point">
    <property name="x" value="-20"/>
    <property name="y" value="0"/>
  </bean>
  
  <bean id="pointC" class="org.saisindhu.package9.Point">
    <property name="x" value="20"/>
    <property name="y" value="0"/>
  </bean>
  
  <bean id="circle" class="org.saisindhu.package9.Circle">
     <property name="centre" ref="pointA"/>
  </bean>
  
</beans>